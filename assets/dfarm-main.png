<svg width="600" height="400" viewBox="0 0 600 400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#126948;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#79ba78;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#75B89F;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="600" height="400" fill="url(#bg)"/>
  
  <!-- Farm Building -->
  <rect x="100" y="180" width="200" height="120" fill="#f4e9b1" stroke="#9E9E9E" stroke-width="2"/>
  <polygon points="90,180 200,120 310,180" fill="#f3cb52" stroke="#9E9E9E" stroke-width="2"/>
  
  <!-- Door -->
  <rect x="170" y="240" width="60" height="60" fill="#126948"/>
  <circle cx="215" cy="270" r="3" fill="#f4e9b1"/>
  
  <!-- Windows -->
  <rect x="120" y="200" width="30" height="25" fill="#ffffff" stroke="#9E9E9E" stroke-width="1"/>
  <rect x="250" y="200" width="30" height="25" fill="#ffffff" stroke="#9E9E9E" stroke-width="1"/>
  
  <!-- IoT Sensors -->
  <circle cx="150" cy="160" r="8" fill="#f3cb52"/>
  <circle cx="250" cy="160" r="8" fill="#f3cb52"/>
  <line x1="150" y1="160" x2="150" y2="140" stroke="#9E9E9E" stroke-width="2"/>
  <line x1="250" y1="160" x2="250" y2="140" stroke="#9E9E9E" stroke-width="2"/>
  
  <!-- Chickens -->
  <ellipse cx="130" cy="290" rx="8" ry="6" fill="#ffffff"/>
  <circle cx="127" cy="287" r="3" fill="#ffffff"/>
  <ellipse cx="160" cy="285" rx="8" ry="6" fill="#ffffff"/>
  <circle cx="157" cy="282" r="3" fill="#ffffff"/>
  <ellipse cx="270" cy="290" rx="8" ry="6" fill="#ffffff"/>
  <circle cx="267" cy="287" r="3" fill="#ffffff"/>
  
  <!-- Control Panel/Dashboard -->
  <rect x="350" y="150" width="180" height="120" fill="#ffffff" stroke="#9E9E9E" stroke-width="2" rx="10"/>
  <rect x="365" y="165" width="150" height="20" fill="#126948"/>
  <text x="440" y="178" text-anchor="middle" fill="white" font-family="Arial" font-size="12" font-weight="bold">D'Farm Dashboard</text>
  
  <!-- Data Visualization Lines -->
  <polyline points="370,200 390,210 410,195 430,205 450,190 470,200 490,185 510,195" 
            fill="none" stroke="#79ba78" stroke-width="3"/>
  <polyline points="370,220 390,225 410,215 430,228 450,210 470,220 490,205 510,218" 
            fill="none" stroke="#f3cb52" stroke-width="3"/>
  
  <!-- Status Indicators -->
  <circle cx="380" cy="245" r="5" fill="#79ba78"/>
  <circle cx="410" cy="245" r="5" fill="#79ba78"/>
  <circle cx="440" cy="245" r="5" fill="#f3cb52"/>
  <circle cx="470" cy="245" r="5" fill="#79ba78"/>
  
  <!-- Project Title -->
  <text x="300" y="50" text-anchor="middle" fill="white" font-family="Arial" font-size="24" font-weight="bold">D'Farm Dream</text>
  <text x="300" y="75" text-anchor="middle" fill="white" font-family="Arial" font-size="14">IoT-based Poultry Management System</text>
  
  <!-- Management Icons -->
  <rect x="450" y="320" width="40" height="30" fill="#f4e9b1" stroke="#9E9E9E" stroke-width="1" rx="5"/>
  <text x="470" y="338" text-anchor="middle" fill="#126948" font-family="Arial" font-size="8" font-weight="bold">PM</text>
  
  <rect x="500" y="320" width="40" height="30" fill="#f4e9b1" stroke="#9E9E9E" stroke-width="1" rx="5"/>
  <text x="520" y="338" text-anchor="middle" fill="#126948" font-family="Arial" font-size="8" font-weight="bold">QA</text>
</svg>
